    <actions name="MediaSizeActions">
        <keyword>Custom</keyword>
        <action selector="launchDialog:" name="CustomSize">
            <parameters>
                <mappings>
                    <mapping name="MediaWidthEdit" feature="XRMediaWidth"/>
                    <mapping name="MediaWidthSpin" feature="XRMediaWidth"/>
                    <mapping name="MediaHeightEdit" feature="XRMediaHeight"/>
                    <mapping name="MediaHeightSpin" feature="XRMediaHeight"/>
                </mappings>
            </parameters>
        </action>
    </actions>
    <dialog name="EarthSmartSettings" helpBookPage="earth_smart.html">
        <dialogSetup name="_DialogSetupInformation_" title="Earth Smart Settings">
            <class>NSView</class>
            <frame>{600,200 }</frame>
            <placement>center</placement>
            <xOrigin>200</xOrigin>
            <validation>earthSmartValidation:</validation>
        </dialogSetup>
        <contentRow name="GreenControllersMimic" origin="{2, 10}" frame="{50,100}">
            <control class="UIImageViewController" frame="{50,150}" size="normal" xOrigin="10" align="leftmost">
                <images>
                    <image image="EarthSmart_24x130_0.pdf" frame="{25, 25}">
                        <states>
                            <state>
                                <feature keyword="Duplex">None</feature>
                                <feature keyword="XRBannerSheet">AtStart</feature>
                                <feature keyword="XRDraftMode">False</feature>
                            </state>
                        </states>
                    </image>
                    <image image="EarthSmart_24x130_1.pdf" frame="{25, 25}">
                        <states>
                            <state>
                                <feature keyword="Duplex">None</feature>
                                <feature keyword="XRBannerSheet">AtStart</feature>
                                <feature keyword="XRDraftMode">True</feature>
                            </state>
                        </states>
                    </image>
                    <image image="EarthSmart_24x130_1.pdf" frame="{25, 25}">
                        <states>
                            <state>
                                <feature keyword="Duplex">None</feature>
                                <feature keyword="XRBannerSheet">None</feature>
                                <feature keyword="XRDraftMode">False</feature>
                            </state>
                        </states>
                    </image>
                    <image image="EarthSmart_24x130_1.pdf" frame="{25, 25}">
                        <states>
                            <state operator="or">
                                <feature keyword="Duplex">DuplexNoTumble</feature>
                                <feature keyword="Duplex">DuplexTumble</feature>
                            </state>
                            <state>
                                <feature keyword="XRBannerSheet">AtStart</feature>
                                <feature keyword="XRDraftMode">False</feature>
                            </state>
                        </states>
                    </image>
                    <image image="EarthSmart_24x130_3.pdf" frame="{25, 25}">
                        <states>
                            <state operator="or">
                                <feature keyword="Duplex">DuplexTumble</feature>
                                <feature keyword="Duplex">DuplexNoTumble</feature>
                            </state>
                            <state>
                                <feature keyword="XRBannerSheet">None</feature>
                                <feature keyword="XRDraftMode">False</feature>
                            </state>
                        </states>
                    </image>
                    <image image="EarthSmart_24x130_3.pdf" frame="{25, 25}">
                        <states>
                            <state>
                                <feature keyword="Duplex">None</feature>
                                <feature keyword="XRBannerSheet">None</feature>
                                <feature keyword="XRDraftMode">True</feature>
                            </state>
                        </states>
                    </image>
                    <image image="EarthSmart_24x130_3.pdf" frame="{25, 25}">
                        <states>
                            <state operator="or">
                                <feature keyword="Duplex">DuplexTumble</feature>
                                <feature keyword="Duplex">DuplexNoTumble</feature>
                            </state>
                            <state>
                                <feature keyword="XRBannerSheet">AtStart</feature>
                                <feature keyword="XRDraftMode">True</feature>
                            </state>
                        </states>
                    </image>
                    <image image="EarthSmart_24x130_5.pdf" frame="{25, 25}">
                        <states>
                            <state operator="or">
                                <feature keyword="Duplex">DuplexNoTumble</feature>
                                <feature keyword="Duplex">DuplexTumble</feature>
                            </state>
                            <state>
                                <feature keyword="XRBannerSheet">None</feature>
                                <feature keyword="XRDraftMode">True</feature>
                            </state>
                        </states>
                    </image>
                </images>
            </control>
        </contentRow>
        <contentRow name="EarthFriendlyDuplexMatrix">
            <control class="UIMatrixController" frame="{250, 22}" fontSize="11" size="normal" buttonType="NSSwitchButton" matrixMode="NSHighlightModeMatrix" name="EarthFriendlyDuplexMatrix">
                <showTitle>YES</showTitle>
                <items>
                    <item title="2-Sided Print"/>
                </items>
            </control>
        </contentRow>
        <contentRow name="EarthFriendlyBannerMatrix" xOrigin="45" verticalSpacing="20">
            <control class="UIMatrixController" frame="{250, 22}" fontSize="11" size="normal" buttonType="NSSwitchButton" matrixMode="NSHighlightModeMatrix" name="EarthFriendlyBannerMatrix">
                <showTitle>YES</showTitle>
                <items>
                    <item title="No Banner Page">
                        <state>
                            <feature keyword="XRBannerSheet">None</feature>
                        </state>
                        <events>
                            <event type="changeValue">
                                <action type="turnOff">
                                    <states>
                                        <state>
                                            <feature keyword="XRAnnotationOption">None</feature>
                                        </state>
                                    </states>
                                </action>
                            </event>
                            <event type="launchOnTurnOn">
                                <action selector="launchDialog:" name="BannerPageEarthSmart"/>
                            </event>
                        </events>
                    </item>
                </items>
            </control>
        </contentRow>
        <contentRow>
            <control class="UIMatrixController" frame="{250, 22}" horizontalSpacing="180" fontSize="11" size="normal" buttonType="NSSwitchButton" matrixMode="NSHighlightModeMatrix" name="EarthFriendlyAnnotation">
                <showTitle>YES</showTitle>
                <items>
                    <item title="Print ID in Margins - All Pages">
                        <state>
                            <feature keyword="XRBannerSheet">None</feature>
                            <feature keyword="XRAnnotationOption">Standard</feature>
                            <feature keyword="XRAnnotationPage">AllPages</feature>
                        </state>
                        <events>
                            <event type="changeValue">
                                <action type="turnOff">
                                    <states>
                                        <state>
                                            <feature keyword="XRAnnotationOption">None</feature>
                                        </state>
                                    </states>
                                </action>
                            </event>
                        </events>
                    </item>
                </items>
                <disable>
                    <state operator="or">
                        <feature keyword="XRBannerSheet">AtStart</feature>
