<?xml version="1.0" encoding="UTF-8"?>
<data>
    <pdeConfiguration>
        <contentRow name="PDEView">
            <control class="NSView" frame="{420, 380}" origin="{0,0}" name="PDEView"/>
        </contentRow>
        <contentRow name="PanelPopup">
            <control class="NSPopUpButton" frame="{250, 26}" origin="{90,350}" name="PDEPanelPopup"/>
        </contentRow>
        <contentRow name="PanelTabview">
            <control class="NSTabView" frame="{420, 326}" origin="{0,20}" type="NSNoTabsBezelBorder" drawsBackground="YES" name="PDETabView">
                <items>
                    <item panel="Paper/Output"/>
                    <item panel="Advanced"/>
                </items>
            </control>
        </contentRow>
        <contentRow name="PrinterStatusView" origin="{10, 0}">
            <contentRow frame="{410, 26}">
                <control class="UIBevelButtonController" size="small" origin="{375, .5}">
                    <toolTip>Earth Smart Settings</toolTip>
                    <images>
                        <image image="EarthSmart_16_0.pdf" frame="{25, 25}">
                            <states>
                                <state>
                                    <feature keyword="Duplex">None</feature>
                                    <feature keyword="XRBannerSheet">AtStart</feature>
                                    <feature keyword="XRDraftMode">False</feature>
                                </state>
                            </states>
                        </image>
                        <image image="EarthSmart_16_1.pdf" frame="{25, 25}">
                            <states>
                                <state>
                                    <feature keyword="Duplex">None</feature>
                                    <feature keyword="XRBannerSheet">AtStart</feature>
                                    <feature keyword="XRDraftMode">True</feature>
                                </state>
                            </states>
                        </image>
                        <image image="EarthSmart_16_1.pdf" frame="{25, 25}">
                            <states>
                                <state>
                                    <feature keyword="Duplex">None</feature>
                                    <feature keyword="XRBannerSheet">None</feature>
                                    <feature keyword="XRDraftMode">False</feature>
                                </state>
                            </states>
                        </image>
                        <image image="EarthSmart_16_1.pdf" frame="{25, 25}">
                            <states>
                                <state operator="or">
                                    <feature keyword="Duplex">DuplexNoTumble</feature>
                                    <feature keyword="Duplex">DuplexTumble</feature>
                                </state>
                                <state>
                                    <feature keyword="XRBannerSheet">AtStart</feature>
                                    <feature keyword="XRDraftMode">False</feature>
                                </state>
                            </states>
                        </image>
                        <image image="EarthSmart_16_3.pdf" frame="{25, 25}">
                            <states>
                                <state operator="or">
                                    <feature keyword="Duplex">DuplexTumble</feature>
                                    <feature keyword="Duplex">DuplexNoTumble</feature>
                                </state>
                                <state>
                                    <feature keyword="XRBannerSheet">None</feature>
                                    <feature keyword="XRDraftMode">False</feature>
                                </state>
                            </states>
                        </image>
                        <image image="EarthSmart_16_3.pdf" frame="{25, 25}">
                            <states>
                                <state>
                                    <feature keyword="Duplex">None</feature>
                                    <feature keyword="XRBannerSheet">None</feature>
                                    <feature keyword="XRDraftMode">True</feature>
                                </state>
                            </states>
                        </image>
                        <image image="EarthSmart_16_3.pdf" frame="{25, 25}">
                            <states>
                                <state operator="or">
                                    <feature keyword="Duplex">DuplexTumble</feature>
                                    <feature keyword="Duplex">DuplexNoTumble</feature>
                                </state>
                                <state>
                                    <feature keyword="XRBannerSheet">AtStart</feature>
                                    <feature keyword="XRDraftMode">True</feature>
                                </state>
                            </states>
                        </image>
                        <image image="EarthSmart_16_5.pdf" frame="{25, 25}">
                            <states>
                                <state operator="or">
                                    <feature keyword="Duplex">DuplexNoTumble</feature>
                                    <feature keyword="Duplex">DuplexTumble</feature>
                                </state>
                                <state>
                                    <feature keyword="XRBannerSheet">None</feature>
                                    <feature keyword="XRDraftMode">True</feature>
                                </state>
                            </states>
                        </image>
                    </images>
                    <events>
                        <event type="onClick">
                            <action selector="launchDialog:" name="EarthSmartSettings"/>
                        </event>
                    </events>
                </control>
            </contentRow>
        </contentRow>
        <onPrint>
            <actions>
                <action selector="launchDialog:" name="Secure">
                    <states>
                        <state>
                            <feature keyword="XRJobType">Secure</feature>
                            <feature keyword="XRJobPassword">""</feature>
                        </state>
                    </states>
                </action>
            </actions>
            <settings>
                <test>
                    <states>
                        <state>
                            <feature keyword="XRDraftMode">True</feature>
                        </state>
                    </states>
                    <set>
                        <feature keyword="XRTonerSaver">Standard</feature>
                    </set>
                </test>
            </settings>
        </onPrint>
    </pdeConfiguration>
    <panel name="Paper/Output" helpBookPage="paper_output.html">
        <contentRow name="JobTypes">
            <label align="center">Job Type:</label>
            <control class="UIPopupButtonController" frame="{175, 36}" size="normal" name="JobTypePopup" feature="XRJobType">
                <items>
                    <item keyword="Normal" image="JobType_24_NormalPrint.pdf"/>
                    <item keyword="Secure" image="JobType_24_SecurePrint.pdf">
                        <events>
                            <event type="onClick">
                                <action selector="launchDialog:" name="Secure"/>
                            </event>
                        </events>
                    </item>
                    <item keyword="Delay" image="JobType_24_DelayPrint.pdf">
                        <events>
                            <event type="onClick">
                                <action selector="launchDialog:" name="Delay"/>
                            </event>
                        </events>
                    </item>
                    <item keyword="Proof" image="JobType_24_ProofPrint.pdf"/>
                    <item keyword="Personal" image="JobType_24_PersonalPrint.pdf"/>
                    <item keyword="Save" image="JobType_24_SaveJob.pdf">
                        <events>
                            <event type="onClick">
                                <action selector="launchDialog:" name="Save"/>
                            </event>
                        </events>
                    </item>
                    </items>
            </control>
            <control class="UIBevelButtonController" frame="{25, 36}" size="normal" title="Ellipsis" name="JobTypeBevel" horizontalSpacing="-2">
                <events>
                    <event type="onClick">
                        <action selector="relaunchDialog:" name="XRJobType"/>
                    </event>
                </events>
                <disable>
                    <state operator="or">
                        <feature keyword="XRJobType">Normal</feature>
                        <feature keyword="XRJobType">Proof</feature>
                        <feature keyword="XRJobType">Hold</feature>
                        <feature keyword="XRJobType">Personal</feature>
                    </state>
                </disable>
            </control>
        </contentRow>
        <contentRow name="PaperSummary">
            <label align="center">Paper:</label>
            <control class="UISummarySelection" frame="{200, 64}" size="normal" name="SummarySelectionWidget">
                <items>
                    <items feature="MediaColor" title="Other Color" actions="MediaColorActions" images="MediaColorImages"/>
                    <items feature="MediaType" title="Other Type" actions="MediaTypeActions" images="MediaTypeImages"/>
                    <item separatorLine="YES"/>
                </items>
                <title>
                    <line label="- " valueOf="MediaColor"/>
                    <line label="- " valueOf="MediaType"/>
                </title>
            </control>
        </contentRow>
        <contentRow name="Duplex">
            <label align="center">2-Sided Printing:</label>
            <control class="UIPopupButtonController" frame="{200, 36}" size="normal" name="DuplexPopup" feature="Duplex">
                <items>
                    <item keyword="None">
                        <images>
                            <image image="2Sided_24_1Sided_Portrait.pdf">
                                <states>
                                    <state>
                                        <feature keyword="XROrientation">Portrait</feature>
                                    </state>
                                </states>
                            </image>
                            <image image="2Sided_24_1Sided_Landscape.pdf">
                                <states>
                                    <state>
                                        <feature keyword="XROrientation">Landscape</feature>
                                    </state>
                                </states>
                            </image>
                        </images>
                    </item>
                    <item keyword="DuplexNoTumble">
                        <images>
                            <image image="2Sided_24_2Sided_Portrait.pdf">
                                <states>
                                    <state>
                                        <feature keyword="XROrientation">Portrait</feature>
                                    </state>
                                </states>
                            </image>
                            <image image="2Sided_24_2Sided_Landscape.pdf">
                                <states>
                                    <state>
                                        <feature keyword="XROrientation">Landscape</feature>
                                    </state>
                                </states>
                            </image>
                        </images>
                    </item>
                    <item keyword="DuplexTumble">
                        <images>
                            <image image="2Sided_24_2SidedFlip_Portrait.pdf">
                                <states>
                                    <state>
                                        <feature keyword="XROrientation">Portrait</feature>
                                    </state>
                                </states>
                            </image>
                            <image image="2Sided_24_2SidedFlip_Landscape.pdf">
                                <states>
                                    <state>
                                        <feature keyword="XROrientation">Landscape</feature>
                                    </state>
                                </states>
                            </image>
                        </images>
                    </item>
                </items>
            </control>
        </contentRow>
        <contentRow name="PrintQuality">
            <label align="center">Print Quality:</label>
            <control class="UIPopupButtonController" frame="{200, 36}" size="normal" name="PrintQualityPopup" feature="XRPrintQuality">
                <items>
                    <item keyword="Standard" image="PrintQuality_24_Standard.pdf"/>
                    <item keyword="Enhanced" image="PrintQuality_24_Enhanced.pdf"/>
                </items>
            </control>
        </contentRow>
    </panel>
    <panel name="Advanced" helpBookPage="advanced.html">
        <contentRow>
            <label align="center">Job Identification:</label>
            <control class="UIPopupButtonController" frame="{225, 26}" fontSize="11" name="BannerSheetPopup">
                <items>
                    <item title="Disable Job ID">
                        <state>
                            <feature keyword="XRBannerSheet">None</feature>
                            <feature keyword="XRAnnotationOption">None</feature>
                        </state>
                    </item>
                    <item title="Print ID on a Banner Sheet">
                        <state>
                            <feature keyword="XRBannerSheet">AtStart</feature>
                            <feature keyword="XRAnnotationOption">None</feature>
                        </state>
                    </item>
                    <item title="Print ID in Margins - All Pages">
                        <state>
                            <feature keyword="XRBannerSheet">None</feature>
                            <feature keyword="XRAnnotationOption">Standard</feature>
                            <feature keyword="XRAnnotationPage">AllPages</feature>
                        </state>
                    </item>
                </items>
            </control>
        </contentRow>
        <contentRow>
            <label align="center">Mirrored Output:</label>
            <control class="UIPopupButtonController" frame="{225, 26}" fontSize="11" name="MirroredOutputPopup" feature="MirrorPrint"/>
        </contentRow>
        <contentRow>
            <label align="center">Negative Output:</label>
            <control class="UIPopupButtonController" frame="{225, 26}" fontSize="11" name="NegativeImagePopup" feature="XRNegativeOutput"/>
        </contentRow>
        <contentRow>
            <label align="center">Draft Mode:</label>
            <control class="UIPopupButtonController" frame="{225, 26}" fontSize="11" name="DraftModePopup" feature="XRDraftMode"/>
        </contentRow>
        <contentRow>
            <control class="UIPushButtonController" frame="{225, 26}" fontSize="11" name="BiDiConfigurationButton" title="Bi-Directional Communication...">
                <events>
                    <event>
                        <action selector="launchDialog:" name="BiDiCommunication"/>
                    </event>
                </events>
            </control>
        </contentRow>
        <contentRow>
            <label align="center">Version:</label>
            <control class="UITextFieldController" frame="{225, 26}" fontSize="11" name="Version" title="VersionBuild">
                <parameters>
                    <variableArguments displayKey="driverVersion,driverBuild">
                        <driverVersion>XRDriverVersion</driverVersion>
                        <driverBuild>XRDriverBuild</driverBuild>
                    </variableArguments>
                </parameters>
            </control>
        </contentRow>
    </panel>
    <dialog name="BiDiCommunication" helpBookPage="bi-di.html">
        <dialogSetup title="Bi-Directional Communication" xOrigin="250" addDefaultButtonRow="YES" addSeparatorLine="YES" growDialog="YES">
            <class>NSView</class>
            <frame>{500, 75}</frame>
            <placement>center</placement>
        </dialogSetup>
        <contentRow>
            <label align="top">Bi-Directional Communication:</label>
            <control class="UIMatrixController" frame="{40,40}" size="normal" buttonType="NSRadioButton" name="Bi-DiCommunicationContext" feature="XRBiDiCommunication" excludeFromKeyboardAccess="YES">
                <disable>
                    <state>
                        <feature keyword="XRPrinterStatus">NotReachable</feature>
                    </state>
                </disable>
                <intercellSpacing>8</intercellSpacing>
                <showTitle>YES</showTitle>
                <items>
                    <item title="On" keyword="True"/>
                    <item title="Off" keyword="False"/>
                </items>
            </control>
        </contentRow>
    </dialog>
    <dialog name="CustomSize" helpBookPage="custom_size.html">
        <dialogSetup title="Custom Size" xOrigin="180">
            <class>NSView</class>
            <frame>{500, 140}</frame>
            <placement>center</placement>
            <controls>
                <control class="UIPopupButtonController" frame="{61, 26}" align="left" size="normal" name="UnitsPopup" feature="XRUnits" ignoresMappings="YES"/>
            </controls>
            <validation>customMediaSizeValidation:</validation>
        </dialogSetup>
        <contentRow>
            <label align="center">Width:</label>
            <control class="UITextFieldController" frame="{50, 22}" size="normal" isEditable="YES" isSelectable="YES" isBezeled="YES" name="MediaWidthEdit" isMeasurement="YES"/>
            <control class="UINumericController" subClass="NSStepper" frame="{19, 27}" size="normal" horizontalSpacing="-1" name="MediaWidthSpin" isMeasurement="YES">
                <increment>1</increment>
                <valueWraps>NO</valueWraps>
                <autoRepeats>YES</autoRepeats>
            </control>
        </contentRow>
        <contentRow>
            <label align="center">Height:</label>
            <control class="UITextFieldController" frame="{50, 22}" size="normal" isEditable="YES" isSelectable="YES" isBezeled="YES" name="MediaHeightEdit" isMeasurement="YES"/>
            <control class="UINumericController" subClass="NSStepper" frame="{19, 27}" size="normal" horizontalSpacing="-1" name="MediaHeightSpin" isMeasurement="YES">
                <increment>1</increment>
                <valueWraps>NO</valueWraps>
                <autoRepeats>YES</autoRepeats>
            </control>
        </contentRow>
    </dialog>
